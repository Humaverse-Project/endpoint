{% extends 'base.html.twig' %}

{% block title %}Competance{% endblock %}

{% block body %}
    <h1>Competance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ competance.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ competance.code }}</td>
            </tr>
            <tr>
                <th>Class</th>
                <td>{{ competance.class }}</td>
            </tr>
            <tr>
                <th>Description_c</th>
                <td>{{ competance.descriptionC }}</td>
            </tr>
            <tr>
                <th>Description_l</th>
                <td>{{ competance.descriptionL }}</td>
